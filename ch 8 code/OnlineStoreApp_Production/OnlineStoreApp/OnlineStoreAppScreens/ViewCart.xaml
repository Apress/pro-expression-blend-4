<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:pc="http://schemas.microsoft.com/prototyping/2010/controls" xmlns:local="clr-namespace:OnlineStoreAppScreens"
	mc:Ignorable="d"
	x:Class="OnlineStoreAppScreens.ViewCart"
	Width="640" Height="480">

	<Grid x:Name="LayoutRoot" Background="White">
		<pc:SketchRectangleSL Margin="19,192,23,197" Style="{StaticResource Rectangle-Sketch}"/>
		<TextBlock HorizontalAlignment="Left" Margin="44,210,0,211" Style="{StaticResource TextBlock-Sketch}" TextWrapping="Wrap" Text="Cart Items" Width="243" FontSize="48" d:LayoutOverrides="Height"/>
		<Button Content="Add to Wish List" HorizontalAlignment="Right" Style="{StaticResource Button-Sketch}" VerticalAlignment="Bottom" Width="206" Margin="0,0,23,49"/>
		<pc:SketchRectangleSL Margin="19,0,259,14" Style="{StaticResource Rectangle-Sketch}" Background="#FFCFD3E5" Height="164" VerticalAlignment="Bottom"/>
		<local:NavSystem Margin="8,8,151,0" VerticalAlignment="Top" d:IsPrototypingComposition="True"/>
	</Grid>
</UserControl>